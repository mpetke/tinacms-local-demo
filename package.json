{
  "name": "tinacms-demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "build": "tinacms build && next build",
    "tina-dev-for-build": "TINA_PUBLIC_IS_LOCAL=false TINA_PUBLIC_CONTENT_API_URL=http://localhost:4001/graphql tinacms dev",
    "build-next": "TINA_PUBLIC_IS_LOCAL=false TINA_PUBLIC_CONTENT_API_URL=http://localhost:4001/graphql next build",
    "build-tina": "TINA_PUBLIC_IS_LOCAL=false tinacms build && cp public/admin/index.html /index.html.prod",
    "hackipv4": "socat TCP4-LISTEN:4001,fork TCP6:[::1]:4001 &",
    "start-tina-prod": "TINA_PUBLIC_IS_LOCAL=false tinacms dev",
    "start-next-prod": " cp /index.html.prod public/admin/index.html && TINA_PUBLIC_IS_LOCAL=false next start",
    "export": "npm run build && next export",
    "lint": "biome lint",
    "dev:build": "next build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "1.10.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.16.0",
    "@types/react": "^18.3.23",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@csstools/selector-resolve-nested": "^3.1.0",
    "@csstools/selector-specificity": "^5.0.0",
    "@graphql-codegen/plugin-helpers": "5.1.1",
    "@headlessui/react": "^2.2.4",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/datalayer": "^1.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "fs-extra": "9.0.1",
    "lucide-react": "^0.484.0",
    "mermaid": "^11.8.0",
    "mongodb": "^4.12.1",
    "mongodb-level": "^0.0.4",
    "motion": "^12.23.0",
    "next": "15.4.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "postcss-selector-parser": "7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-use-measure": "^2.1.7",
    "scheduler": "^0.20.0",
    "shiki": "^3.7.0",
    "styled-jsx": "^5.1.7",
    "sucrase": "^3.35.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "tinacms": "2.8.1",
    "tinacms-authjs": "^11.0.1",
    "tinacms-gitprovider-github": "^3.0.0",
    "tw-animate-css": "^1.3.5",
    "usehooks-ts": "^3.1.1",
    "yup": "^0.32.0"
  }
}
